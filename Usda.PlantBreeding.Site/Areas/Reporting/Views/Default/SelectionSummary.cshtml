@model Usda.PlantBreeding.Core.Models.Reporting.SelectionSummaryReport

@{
    ViewBag.Title = "Breeding Summary";
    ViewBag.IndexTitle = "Breeding Summary";
}
<div class="row"></div>
<h2>Breeding Summary</h2>
<hr />

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="row">
            <div class="col-md-6">

                <div class="form-group">
                    @Html.LabelFor(model => model.GenotypeId, htmlAttributes: new { @class = "control-label col-md-2" })
                    @Html.TextBoxFor(model => model.GenotypeName, htmlAttributes: new { hidden="hidden"})
                </div>

                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Report" class="btn btn-default" />
                    </div>
                </div>
            </div>
        </div>
    </div>
}